    'Tira o "Checked" dos menus de Idioma
    Uncheck ("Idioma")

    Select Case Index
    
        Case 0
            mnuIdioma(0).Checked = True
            TrocarIdioma ("Ptb")
        
        Case 1
            mnuIdioma(1).Checked = True
            TrocarIdioma ("Eng")
            
    End Select
    
    'Coloca o novo Rótulo em "cmdNovoJogo"
    cmdNovoJogo.Caption = cmdNovoJogoTEXTO(cmdNovoJogoSTATUS)
    
End Sub

