Private Sub mnuNovoJogo_Click()
'Primeiramente, limpa a tela de Jogo e, depois, inicia um
'novo Jogo, habilitando o Timer ("TimerJogo")

    Dim Contador As Integer
    
    'Limpa as variáveis
    TipoBlocoProx = 0
    EstiloBlocoProx = 0
    NumBloco = 0
    
    'Atribue os valores iniciais aos rótulos
    lblPontos = "0"
    cmdNovoJogo.Caption = cmdNovoJogoTEXTO(0)
    
    'Volta os blocos às posições iniciais e sem figuras anexas
    
    Contador = 0

    'Blocos de "pctJogo"
    Do While Contador < 180
    
        With imgBloco(Contador)
            .Top = 0
            .Left = 0
            .Visible = False
            .Picture = Nothing
        End With
    
        Contador = Contador + 1
    
    Loop

    'Inicia o Jogo
    IniciarJogo
    
End Sub

